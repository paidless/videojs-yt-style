  "bugs": {
    "url": "https://github.com/liriliri/eruda-orientation/issues"
  },
  "homepage": "https://github.com/liriliri/eruda-orientation#readme",
  "devDependencies": {
    "eruda": "^2.0.0",
    "eslint": "^4.12.1",
    "nws": "^1.1.1",
    "prettier": "^1.14.2",
    "uglify-js": "^3.2.0"
  }
}
